{
  "name": "@vue-deckgl-suite/layers",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/vue-deckgl-suite-layers.umd.js",
  "module": "dist/vue-deckgl-suite-layers.es.js",
  "types": "dist/main.d.ts",
  "web-types": "dist/web-types.json",
  "files": [
    "dist/**/*",
    "src/**/*"
