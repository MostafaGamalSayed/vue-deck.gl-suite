{
  "name": "@vue-deckgl-suite/maplibre",
  "description": "A Vue 3 package integrating Deck.gl with MapLibre for geospatial visualizations",
  "version": "0.1.0-alpha.0",
  "author": {
    "name": "Mostafa Gamal",
    "email": "mostafagamal99418@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/vue-deckgl-suite-maplibre.umd.js",
  "module": "dist/vue-deckgl-suite-maplibre.es.js",
  "types": "dist/main.d.ts",
  "web-types": "dist/web-types.json",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build && vue-docgen-web-types --configFile web-types.config.cjs",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vue-tsc": "^2.2.0",
    "vue": "^3.5.13",
    "maplibre-gl": "^5.1.0",
    "@deck.gl/core": "^9.1.3",
    "@deck.gl/mapbox": "^9.1.3",
    "vite-plugin-dts": "^4.5.0",
    "vue-docgen-web-types": "^0.1.8"

  },
  "peerDependencies": {
    "@deck.gl/core": "^9.1.3",
    "@deck.gl/mapbox": "^9.1.3",
    "vue": "^3.5.13",
    "maplibre-gl": "^5.1.0"
  }
}
